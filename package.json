{
  "name": "elven-tools",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "version": "1.18.0",
  "elvenTools": {
    "smartContractVersionTagName": "v1.12.0",
    "minterDappVersionTagName": "v6.0.0"
  },
  "description": "Interacting with custom NFT related Smart Contracts on the MultiversX (Elrond) blockchain",
  "bin": {
    "elven-tools": "./build/index.js"
  },
  "type": "module",
  "main": "build/index.js",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "default": "./build/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "generate": "npm run build && node build/index.js",
    "build": "rimraf build && node ./esbuild.config.js && tsc",
    "dev:lint": "eslint src/** --fix",
    "dev:prettier": "prettier --write 'src/**/*.{js,ts,json}'",
    "prepublishOnly": "npm run build"
  },
  "author": "Julian Ä†wirko <julian.io>",
  "license": "MIT",
  "homepage": "https://www.elven.tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/ElvenTools/elven-tools-cli"
  },
  "keywords": [
    "elrond",
    "multiversx",
    "blockchain",
    "sdk",
    "javascript",
    "nft",
    "cli"
  ],
  "devDependencies": {
    "@types/cross-spawn": "6.0.2",
    "@types/decompress": "^4.2.4",
    "@types/node": "18.15.0",
    "@types/prompts": "2.4.2",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "esbuild": "0.17.11",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.8.4",
    "rimraf": "4.4.0",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@multiversx/sdk-core": "11.4.1",
    "@multiversx/sdk-network-providers": "1.3.0",
    "@multiversx/sdk-wallet": "3.0.0",
    "axios": "1.3.4",
    "bignumber.js": "9.1.1",
    "cosmiconfig": "8.1.0",
    "cross-fetch": "3.1.5",
    "cross-spawn": "7.0.3",
    "decompress": "^4.2.1",
    "get-stream": "^6.0.1",
    "got": "^12.6.0",
    "ora": "6.1.2",
    "p-event": "^5.0.1",
    "p-throttle": "5.0.0",
    "prompts": "2.4.2"
  }
}
